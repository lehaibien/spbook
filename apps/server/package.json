    "@elysiajs/openapi": "^1.4.11",
    "@elysiajs/opentelemetry": "^1.4.8",
    "@elysiajs/swagger": "^1.3.1",
    "@elysiajs/trpc": "^1.1.0",
    "@spbook/api": "workspace:*",
    "@spbook/auth": "workspace:*",
    "@spbook/db": "workspace:*",
    "@trpc/server": "catalog:",
    "better-auth": "catalog:",
    "dotenv": "catalog:",
    "elysia": "catalog:",
    "logixlysia": "^5.3.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "typescript": "catalog:",
    "@types/bun": "^1.2.6",
    "tsdown": "^0.16.5",
    "@spbook/config": "workspace:*"
  }
